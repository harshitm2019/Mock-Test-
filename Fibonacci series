public class Test {

     public static void main(String[] args) {
        
        int n = 8;
        int x = 0 , y = 1;

        System.out.print(x);
        System.out.print(" " + y);
    
        for(int i = 2;i < n;i++){
 
            int temp = x + y;
            System.out.print(" " + temp);
            x = y;
            y = temp;

        }

     }
}
